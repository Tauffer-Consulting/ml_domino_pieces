    "TrainTestSplitPiece": {
        "name": "TrainTestSplitPiece",
        "dependency": {
            "dockerfile": null,
            "requirements_file": "requirements_0.txt"
        },
        "tags": [
            "preprocessing",
            "datasets"
        ],
        "style": {
            "node_label": "Split Data",
            "node_type": "default",
            "node_style": {
                "backgroundColor": "#b3cde8"
            },
            "useIcon": true,
            "icon_class_name": "icon-park-outline:split",
            "iconStyle": {
                "cursor": "pointer"
            }
        },
        "description": "Split data into train and test sets",
        "input_schema": {
            "$defs": {
                "OutputType": {
                    "enum": [
                        "file",
                        "object"
                    ],
                    "title": "OutputType",
                    "type": "string"
                }
            },
            "description": "Input data for TextSummarizerPiece",
            "properties": {
                "data": {
                    "anyOf": [
                        {
                            "items": {
                                "type": "object"
                            },
                            "type": "array"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The data to be split.",
                    "from_upstream": "always",
                    "title": "Data"
                },
                "data_path": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The path to the data to be split.",
                    "from_upstream": "always",
                    "title": "Data Path"
                },
                "test_data_size": {
                    "default": 0.8,
                    "description": "The size (%) of the test data.",
                    "title": "Test Data Size",
                    "type": "number"
                },
                "random_state": {
                    "default": 42,
                    "description": "The random state for the split.",
                    "title": "Random State",
                    "type": "integer"
                },
                "output_type": {
                    "allOf": [
                        {
                            "$ref": "#/$defs/OutputType"
                        }
                    ],
                    "default": "object",
                    "description": "The output type. Use file for large datasets.",
                    "title": "Output Type"
                }
            },
            "title": "InputModel",
            "type": "object"
        },
        "output_schema": {
            "description": "Output data for TextSummarizerPiece",
            "properties": {
                "train_data": {
                    "anyOf": [
                        {
                            "items": {
                                "type": "object"
                            },
                            "type": "array"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Train Data"
                },
                "test_data": {
                    "anyOf": [
                        {
                            "items": {
                                "type": "object"
                            },
                            "type": "array"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Test Data"
                },
                "train_data_path": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Train Data Path"
                },
                "test_data_path": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Test Data Path"
                }
            },
            "title": "OutputModel",
            "type": "object"
        },
        "secrets_schema": null,
        "source_url": "https://github.com/Tauffer-Consulting/ml_domino_pieces/tree/main/pieces/TrainTestSplitPiece"
    },
